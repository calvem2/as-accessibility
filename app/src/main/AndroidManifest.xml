<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cse340.askforhelp">

    <uses-sdk
        android:maxSdkVersion="28" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:icon="@mipmap/icon"
        android:roundIcon="@mipmap/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:supportsRtl="true">
        <receiver android:name=".MyReceiver" android:exported="false"/>
        <activity android:name=".ApacheLicenseActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".InfoActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".ChangeContactGroupsActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".ChangeRequestsActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".SettingsActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".ChooseContactGroupsActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".ChooseRequestsActivity"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
    </application>

</manifest>
